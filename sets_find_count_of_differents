
def count_distinct_stamps(stamp_names):
    distinct_stamps = set()  # Use a set for efficient membership checks
    for stamp_name in stamp_names:
        distinct_stamps.add(stamp_name)
    return len(distinct_stamps)


num_stamps = int(input())
stamp_names = [input() for _ in range(num_stamps)]

distinct_count = count_distinct_stamps(stamp_names)
print(distinct_count)
